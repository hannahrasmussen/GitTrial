   "source": [
    "#Circumference and area of a circle, surface area and volume of a sphere, surface area and volume of a cube\n",
    "\n",
    "def C(r):\n",
    "    return 2*np.pi*r\n",
    "\n",
    "def A(r):\n",
    "    return np.pi*r**2\n",
    "\n",
    "def vol_cube(s):\n",
    "    return s**3\n",
    "\n",
    "def vol_sphere(r):\n",
    "    return (4/3)*np.pi*r**3\n",
    "\n",
    "def vol_cylinder(r,h):\n",
    "    return h*A(r)"
   ]
